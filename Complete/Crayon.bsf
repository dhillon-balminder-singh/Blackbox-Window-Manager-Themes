 *.appearance: raised solid border
 *.backgroundColor: light blue
 *.alignment: center
 window.label.focus.backgroundColor: #3C7EC8
 window.grip.focus.backgroundColor: #3C7EC8
 *button.pressed.appearance: sunken solid
*button.pressed.backgroundColor: #A55E5F
*.button.*.backgroundColor: brown
*.button.unfocus.backgroundColor: #A55E5F
window.label.focus.textColor: white
window.label.unfocus.textColor: dark grey
*.foregroundColor: azure
menu.title.backgroundColor: #3C7EC8
menu.active.backgroundColor: #3C7EC8
menu.frame.foregroundColor: black
toolbar.windowLabel.backgroundColor: #3C7EC8
toolbar.windowLabel.textColor: white
*font: Lucida Typewriter-12:Bold