#COMMON KEYS#
*.appearance: raised gradient vertical border
*.color1: #FFFFFF
*.color2: #000000
*.borderWidth: 1
*.borderColor: #000000
*.font: tahoma-12:Bold
*.alignment: center

#WINDOW APPEARANCE KEYS#

window.title.*.appearance: raised gradient vertical border

	#window.title.focus.color1: #FFFFFF
	#window.title.focus.color2: #000000
	window.title.unfocus.color1: #FFFFFF
	window.title.unfocus.color2: #333333

#window.button.*.appearance: flat solid border
	#window.button.*.backgroundColor: #737173
	#window.button.*.foregroundColor: #FFFFFF
	
window.button.*.appearance: flat gradient vertical border
	window.button.*.color1: #5A595A
	window.button.*.color2: #393839
	window.button.*.foregroundColor: #FFFFFF

window.button.*.pressed.appearance: sunken gradient vertical border
	window.button.*.pressed.color1: #5A595A
	window.button.*.pressed.color2: #393839
	
window.label.*.appearance: parentrelative
	window.label.*.textColor: #FFFFFF

#MENU APPEARANCE KEYS#

menu.frame.appearance: flat solid border
	menu.frame.backgroundColor: #FFFFFF
	menu.frame.textColor: #737173
	menu.frame.foregroundColor: #737173
	#menu.frame.marginWidth: 0
	
menu.active.appearance: flat solid
	menu.active.backgroundColor: #737173
	menu.active.foregroundColor: #FFFFFF
	#menu.active.marginWidth: 0

menu.frame.disabledColor: light grey

#TOOLBAR APPEARANCE KEYS#

toolbar.appearance: raised gradient vertical border
toolbar.label.appearance: parentrelative
toolbar.windowLabel.appearance: parentrelative
toolbar.clock.appearance: parentrelative
toolbar.*.textColor: #FFFFFF

#toolbar.button.appearance: flat solid border
	#toolbar.button.backgroundColor: #737173
	#toolbar.button.foregroundColor: #FFFFFF
	
toolbar.button.appearance: flat gradient vertical border
	toolbar.button.color1: #5A595A
	toolbar.button.color2: #393839
	toolbar.button.foregroundColor: #FFFFFF

toolbar.button.pressed.appearance: sunken gradient vertical border
	toolbar.button.pressed.color1: #5A595A
	toolbar.button.pressed.color2: #393839